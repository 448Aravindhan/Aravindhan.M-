n1 = int(input("Enter the number 1: "))
n2 = int(input("Enter the number 2 : "))
n3 = int(input("Enter the number 3 : "))
if n1 > n2 and n1 > n3: 
    print('largest number of three number is=',n1)
elif n2>n1 and n2 > n3:
	print('largest number of three number is=',n2)
else:
	print('largest number of three number is=',n3)
